<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.cathouse.app.model.User">
        <table name="user" schema="cat_house" catalog=""/>
        <attributes>
            <id name="id">
                <column name="id" precision="11"/>
            </id>
            <basic name="name">
                <column name="name" length="50"/>
            </basic>
            <basic name="userName">
                <column name="user_name" length="30"/>
            </basic>
            <basic name="password">
                <column name="password" length="30"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
            <basic name="phoneNumber">
                <column name="phone_number" length="15"/>
            </basic>
            <basic name="createdDate">
                <column name="created_date" nullable="false" precision="-1"/>
            </basic>
            <basic name="updatedDate">
                <column name="updated_date" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
